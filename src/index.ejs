<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <title>webpack project</title>
    <% if (!!mobile) { %>
        <meta name="viewport"
              content="width=device-width;initial-scale=1.0;maximum-scalable=1.0;minimum-scalable=1.0;user-scalable=yes;"
        />
    <% } %>
    <% if (mobile.includes('rem')) { %>
        <script><%= require('!raw-loader?esModule=false!babel-loader!lib-flexible/flexible') %></script>
    <% } %>
</head>
<body>
<main id="root">
</main>
<% if(vendors) { %>
    <script type="text/javascript" src=<%= vendors %>></script>
<% } %>
</body>
</html>